{
  "name": "scroll-zoom",
  "version": "0.1.1",
  "author": "Armin Sebastian",
  "repository": "https://github.com/dessant/scroll-zoom",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=development gulp build",
    "build:opera": "NODE_ENV=development TARGET_ENV=opera gulp build",
    "build:chrome": "NODE_ENV=development TARGET_ENV=chrome gulp build",
    "build:prod": "NODE_ENV=production gulp build",
    "build:prod:opera": "NODE_ENV=production TARGET_ENV=opera gulp build",
    "build:prod:chrome": "NODE_ENV=production TARGET_ENV=chrome gulp build",
    "build:prod:zip": "yarn run build:prod && yarn run zip",
    "build:prod:zip:opera": "yarn run build:prod:opera && yarn run zip",
    "build:prod:zip:chrome": "yarn run build:prod:chrome && yarn run zip",
    "exec": "yarn run build && yarn run start",
    "exec:prod": "yarn run build:prod && yarn run start",
    "zip": "web-ext build -s dist/ -a artifacts/",
    "start": "web-ext run -s dist/",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "push": "git push --follow-tags origin master",
    "release": "standard-version && yarn run push"
  },
  "browserslist": [
    "Chrome >= 55",
    "Firefox >= 58",
    "Opera >= 42"
  ],
  "dependencies": {
    "@material/theme": "^0.30.0",
    "@material/typography": "^0.28.0",
    "ext-components": "dessant/ext-components#^0.1.3",
    "storage-versions": "dessant/storage-versions#^0.1.1",
    "typeface-roboto": "^0.0.54",
    "vue": "^2.5.13",
    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-loader": "^7.1.3",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-jsonmin": "^1.1.0",
    "gulp-merge-json": "^1.2.1",
    "gulp-postcss": "^7.0.1",
    "gulp-sequence": "^1.0.0",
    "node-sass": "^4.7.2",
    "npm-check-updates": "^2.14.1",
    "recursive-readdir": "^2.2.2",
    "sass-loader": "^6.0.6",
    "standard-version": "^4.3.0",
    "svg2png": "^4.1.1",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "web-ext": "^2.4.0",
    "webpack": "^3.11.0"
  },
  "private": true
}
